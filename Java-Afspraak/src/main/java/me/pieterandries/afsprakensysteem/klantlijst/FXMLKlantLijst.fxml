<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.pieterandries.afsprakensysteem.klantlijst.FXMLKlantLijstController">
   <children>
      <TableView fx:id="tableView" prefHeight="400.0" prefWidth="600.0">
         <columns>
            <TableColumn fx:id="naamColumn" prefWidth="75.0" text="Naam" />
            <TableColumn fx:id="idColumn" prefWidth="75.0" text="ID" />
            <TableColumn fx:id="telColumn" prefWidth="75.0" text="Telefoonnummer" />
            <TableColumn fx:id="mailColumn" prefWidth="75.0" text="E-Mail" />
            <TableColumn fx:id="woonplaatsColumn" prefWidth="75.0" text="Woonplaats" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox layoutX="200.0" layoutY="400.0" prefHeight="100.0" prefWidth="200.0">
         <children>
            <JFXButton fx:id="verwijderButton" onAction="#verwijderSelectie" prefHeight="106.0" prefWidth="211.0" text="Verwijderen" />
         </children>
      </HBox>
   </children>
</AnchorPane>
